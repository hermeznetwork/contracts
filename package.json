{
  "name": "hermez-contracts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "npx buidler clean && rm -rf cache coverage build node_modules yarn.lock coverage.json .openzeppelin",
    "compile": "npx buidler compile",
    "test": "npx buidler test test/*/*.test.js",
    "test:math": "npx buidler test test/math/*.test.js",
    "test:hermez": "npx buidler test test/hermez/*.test.js",
    "test:auction": "npx buidler test test/auction/*.test.js",
    "test:upgradability": "npx buidler test test/upgradability/*.test.js",
    "test:withdrawalDelayer": "npx buidler test test/withdrawalDelayer/*.test.js",
    "test:gas": "scripts/run-gas-reporter.sh",
    "test:gas:hermez": "scripts/run-gas-reporter.sh test/hermez/*test.js test/hermez/.solcover.js",
    "test:gas:math": "scripts/run-gas-reporter.sh \"test/math/*.test.js\" \"test/math/.solcover.js\"",
    "test:gas:auction": "scripts/run-gas-reporter.sh \"test/auction/*.test.js\" \"test/auction/.solcover.js\"",
    "test:gas:withdrawalDelayer": "scripts/run-gas-reporter.sh test/withdrawalDelayer/*.test.js test/withdrawalDelayer/.solcover.js",
    "test:coverage": "npx buidler coverage --network coverage",
    "test:coverage:math": "npx buidler coverage --testfiles 'test/math/*.test.js' --solcoverjs test/math/.solcover.js --network coverage",
    "test:coverage:hermez": "npx buidler coverage --testfiles 'test/hermez/*.test.js' --solcoverjs test/hermez/.solcover.js --network coverage",
    "test:coverage:auction": "npx buidler coverage --testfiles 'test/auction/*.test.js' --solcoverjs test/auction/.solcover.js --network coverage",
    "test:coverage:withdrawalDelayer": "npx buidler coverage --testfiles 'test/withdrawalDelayer/*.test.js' --solcoverjs test/withdrawalDelayer/.solcover.js --network coverage",
    "lint:sol": "yarn run solhint contracts/*/*.sol"
  },
  "keywords": [],
  "author": "",
  "license": "AGPL-3.0",
  "devDependencies": {
    "@hermeznetwork/commonjs": "0.0.1",
    "@nomiclabs/buidler": "^1.4.5",
    "@nomiclabs/buidler-ethers": "^2.0.0",
    "@nomiclabs/buidler-etherscan": "^1.3.3",
    "@nomiclabs/buidler-solhint": "^1.3.3",
    "@nomiclabs/buidler-waffle": "^2.0.0",
    "@nomiclabs/buidler-web3": "^1.3.4",
    "@openzeppelin/buidler-upgrades": "^1.0.2",
    "@openzeppelin/contracts": "^3.1.0",
    "@openzeppelin/contracts-ethereum-package": "^3.0.0",
    "@openzeppelin/test-helpers": "^0.5.6",
    "@openzeppelin/upgrades": "^2.8.0",
    "buidler-deploy": "^0.5.6",
    "buidler-gas-reporter": "^0.1.3",
    "buidler-spdx-license-identifier": "^1.0.2",
    "chai": "^4.2.0",
    "circom": "^0.5.17",
    "circomlib": "^0.2.4",
    "dotenv": "^8.2.0",
    "eth-gas-reporter": "^0.2.17",
    "ethereum-waffle": "^3.0.2",
    "ethers": "^5.0.12",
    "ffjavascript": "^0.2.10",
    "mocha": "^8.1.1",
    "prettier": "^2.0.5",
    "prettier-plugin-solidity": "1.0.0-alpha.56",
    "solc": "0.6.12",
    "solhint": "^3.2.0",
    "solidity-coverage": "^0.7.9",
    "solidity-docgen": "^0.5.4",
    "web3": "^1.2.11"
  },
  "dependencies": {
    "@nomiclabs/buidler-ganache": "^1.3.3",
    "ganache-cli": "^6.12.1"
  }
}
